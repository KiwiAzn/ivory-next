{
  "name": "ivory",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tailwind": "concurrently 'next dev' 'yarn tailwind --watch'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "yarn tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "daisyui": "^2.38.1",
    "next": "latest",
    "postcss-import": "^15.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "tslib": "^2.4.1",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
